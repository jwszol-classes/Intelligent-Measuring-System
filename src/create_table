CREATE EXTERNAL TABLE IF NOT EXISTS terrain_tiles.table (
  `filename` string,
  `coordinates` string,
  `elevation` string
)
ROW FORMAT SERDE 'org.apache.hadoop.hive.serde2.lazy.LazySimpleSerDe'
WITH SERDEPROPERTIES (
  'serialization.format' = ',',
  'field.delim' = ','
) LOCATION 's3://terrain_tiles/results/'
TBLPROPERTIES ('has_encrypted_data'='false');
